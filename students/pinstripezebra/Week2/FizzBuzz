
for i in range(1,101):
    if i%3 == 0 and i%5 == 0: #prints 'FizzBuzz' if i divisible by 3 and 5
        print('FizzBuzz')
    elif i%3 ==0: #prints 'Fizz' if i divisible by just 3
        print('Fizz')
    elif i%5 ==0: #prints 'Buzz' if i divisible by just 5
        print('Buzz')
    else: #prints i if none of the previous conditions are true
        print(i)
